# Getting Proxy Template
wget -q -O /etc/supotka https://raw.githubusercontent.com/1fuckingscript/version/main/pornhub.py
chmod +x /etc/supotka

service

function service1() {
# Installing Service
cat << END > /etc/systemd/system/supotka.service 
[Unit]
Description=supotka
Documentation=https://google.com
After=network.target nss-lookup.target

[Service]
Type=simple
User=root
CapabilityBoundingSet=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
AmbientCapabilities=CAP_NET_ADMIN CAP_NET_BIND_SERVICE
NoNewPrivileges=true
ExecStart=/usr/bin/python -O /etc/supotka
Restart=on-failure

[Install]
WantedBy=multi-user.target
END
}
service1

function setting() {
sed -i "/DEFAULT_HOST = '127.0.0.1:443'/c\DEFAULT_HOST = '127.0.0.1:550'" /etc/supotka

systemctl daemon-reload
systemctl enable supotka
systemctl restart supotka
}
setting

function remove() {
echo "" > .bash_history 
history -c
echo '' > /var/log/syslog
}
remove



clear

rm /root/.bash_history
echo 'UTUT MU' > /root/.bash_history

echo "BKIT ANG POGI NI MARK? "
